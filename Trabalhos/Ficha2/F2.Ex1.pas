Program IgualOuDiferente ;
var a,b:string;

Begin
    write('Insira um/uma numero/letra: ');
    readln(a);
    write('Insira outro/a numero/letra: ');
    readln(b);
    if (a=b) then 
      writeln('Inseriu dois caracteres iguais')
    else  
      writeln('Inseriu dois caracteres diferentes');
      
End.
