Program Pzim ;
var a,b,c,d,e:integer;
    f,g:real;


Begin
     writeln('Selecione o 1º número');
     readln(a);
     writeln('Selecione o 2º número');
     readln(b);
     c:=a+b;
     d:=a-b;
     e:=a*b;
     f:=a/b;
     writeln('Soma',' ',a,'+',b,' ','=',c);
     writeln('Subtracao',' ',a,'-',b,' ','=',d);
     writeln('Multiplicaçao',' ',a,'X',b,' ','=',e);
     writeln('Divisao',' ',a,'/',b,' ','=',f);
     readln(g);
     
     
     
     
  




End.